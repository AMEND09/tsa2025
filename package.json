    "react-hook-form": "^7.52.1",
    "react-resizable-panels": "^2.0.20",
    "react-router-dom": "^7.1.2",
    "recharts": "^2.15.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8"
