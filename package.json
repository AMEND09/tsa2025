    "postcss": "^8.4.39",
    "react-draggable": "^4.4.6",
    "react-grid-layout": "^1.5.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.4.11",
    "vite-plugin-pages": "^0.32.4",
    "vite-plugin-pwa": "^0.21.1"
  },
  "overrides": {
