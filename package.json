{
  "homepage": "/LandingPage",
  "name": "AgriMind.ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:github": "npm run predeploy && npm run deploy",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:all": "npm run build && npm run deploy",
    "deploy:fullstack:heroku": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Platform heroku",
    "deploy:fullstack:railway": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Platform railway",
    "deploy:fullstack:render": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Platform render",
    "deploy:fullstack": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "build:prod": "npm run build",
    "build:preview": "npm run build && npm run preview",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:fullstack": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Platform build",
    "quick-deploy": "npm run deploy:github",
    "quick-build": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Platform quick",
    "backend:migrate": "python manage.py migrate",
    "backend:collectstatic": "python manage.py collectstatic --noinput",
    "backend:test": "python manage.py test",
    "backend:runserver": "python manage.py runserver",
    "start:dev": "concurrently \"npm run backend:runserver\" \"npm run dev\"",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
